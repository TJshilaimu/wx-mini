.my-top {
  width: 100%;
  height: 280rpx;
  background-color: #feb70f;
  text-align: center;
}

.my-top image {
  width: 180rpx;
  height: 180rpx;
  border-radius: 50%;
}

.my-text {
  display: flex;
  justify-content: center;
  font-size: 48rpx;
  margin-top: 10rpx;
}
.my-text navigator{
  margin-right:20rpx;
}

.section {
  background: #fff;
  margin-bottom: 10rpx;
  width: 100%
}

.section-item {
  position: relative;
  margin: 0 auto;
  padding-top: 10rpx;
  padding-bottom: 10rpx;
  height: 66rpx;
  width: 90%;
  line-height: 66rpx;
  border-bottom: 1rpx solid rgb(206, 199, 199);
}

.section-item::after {
  content: '>';
  display: block;
  position: absolute;
  right: 10rpx;
  top: 10rpx;
}

.section-money {
  padding: 10rpx;
  height: 66rpx;
  line-height: 66rpx;
}

.special {
  padding-left: 3%;
  margin: 0 auto;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-bottom: 10rpx;

}

.money {
  margin-right: 40rpx;
  color: #777
}